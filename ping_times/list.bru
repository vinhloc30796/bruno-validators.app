meta {
  name: ping-times list
  type: http
  seq: 2
  tags: [read]
}

get {
  url: {{base_url}}/ping-times/{{network}}{{filetype}}
}

params:path {
  network: {{network}}
  filetype: {{filetype}}
}

params:query {
  limit: 100
}

headers {
  Token: {{process.env.API_TOKEN}}
  Content-Type: application/json
}
